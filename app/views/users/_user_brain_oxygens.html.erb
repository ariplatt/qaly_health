<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Brain Oxygens

    <a data-toggle="collapse" href="#user_brain_oxygens_new_form">
      Add reading
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @user.brain_oxygens.each do |reading| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <%= link_to reading, reading %>

      </li>
    <% end %>

    <div class="card-footer">
      <div class="collapse" id="user_brain_oxygens_new_form">
        <%= render partial: "users/user_brain_oxygens_new_form" %>
      </div>
    </div>

  </ul>
</div>
