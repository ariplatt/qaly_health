<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Demographics Infos

    <a data-toggle="collapse" href="#user_demographics_infos_new_form">
      Add demographics info
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @user.demographics_infos.each do |demographics_info| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <%= link_to demographics_info, demographics_info %>

      </li>
    <% end %>

    <div class="card-footer">
      <div class="collapse" id="user_demographics_infos_new_form">
        <%= render partial: "users/user_demographics_infos_new_form" %>
      </div>
    </div>

  </ul>
</div>
